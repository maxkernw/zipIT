{
	"manifest_version": 2,
	"name": "Realm Login",
	"description": "This is a helper extension for loging in",
	"version": "1.0",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "beer.png",
		"48": "beer.png",
		"128": "beer.png"
	},
	"background": {
		"scripts": [
			"popup.js"
		],
		"persistent": true
	},
	"permissions": [
		"activeTab","http://*/*", "https://*/*"
	]
}